{
  darwinStateVersion,
  inputs,
  outputs,
  stateVersion,
  ...
}:
let
  builders = import ./flake-builders.nix {
    inherit
      darwinStateVersion
      inputs
      outputs
      stateVersion
      ;
  };
in
{
  inherit (builders)
    mkDarwin
    mkHome
    mkNixos
    forAllSystems
    mkSystemConfig
    generateConfigs
    isLinuxEntry
    isDarwinEntry
    isISOEntry
    isHomeOnlyEntry
    ;
}
