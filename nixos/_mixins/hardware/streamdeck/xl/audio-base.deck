[[keys]]
  index = 0
  [keys.widget]
    id = "button"
    [keys.widget.config]
      icon = "~/Studio/StreamDeck/Icons/Cider-Logo.png"
  [keys.action]
    exec = "cider"

[[keys]]
  index = 1
  [keys.widget]
    id = "button"
    [keys.widget.config]
      icon = "~/Studio/StreamDeck/Icons/Nebula/Audio/volume-down.jpg"
  [keys.action]
    exec = "ciderctl vol 5-"
  [keys.action_hold]
    exec = "ciderctl vol 25"

