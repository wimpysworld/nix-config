    exec = "ciderctl vol 5+"
  [keys.action_hold]
    exec = "ciderctl vol 75"

[[keys]]
  index = 3
  [keys.widget]
    id = "command"
    interval = 2000
    [keys.widget.config]
      command = "echo 'Cider';echo 'Music';ciderctl vol"
      font = "thin;regular;bold"
      color = "#CA1C35;#CA1C35;#fefefe"
  [keys.action]
    exec = "ciderctl vol 50"
  [keys.action_hold]
    exec = "ciderctl vol 100"

